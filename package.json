  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "homepage": "https://github.com/birchill/hikibiki#readme",
  "dependencies": {
    "@birchill/json-equalish": "^1.0.1",
    "@birchill/normal-jp": "^1.1.3",
