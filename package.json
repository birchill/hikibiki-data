  "devDependencies": {
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chai-datetime": "0.0.36",
    "@types/fetch-mock": "^7.3.1",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.11.8",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.5.0",
    "fetch-mock": "9.11.0",
    "husky": "^6.0.0",
    "karma": "^6.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.0.0",
    "karma-mocha": "^2.0.0",
    "karma-webpack": "^5.0.0-alpha.5",
    "lint-staged": "^10.0.1",
    "mocha": "^8.0.1",
    "pinst": "^2.1.4",
    "prettier": "^2.0.2",
    "process": "^0.11.10",
    "rollup": "^2.29.0",
    "sinon": "^10.0.0",
    "standard-version": "^9.0.0",
    "ts-loader": "^8.0.0",
    "typescript": "^4.0.2",
    "util": "^0.12.3",
    "webpack": "^5.17.0"
  },
  "lint-staged": {
